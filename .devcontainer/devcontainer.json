{
  "name": "Flutter Development Environment",
  "dockerFile": "Dockerfile",
  "settings": {
    "terminal.integrated.shell.linux": "/bin/bash"
  },
  "extensions": [
    "Dart-Code.dart-code",
    "Dart-Code.flutter"
  ],
  "forwardPorts": [
    8080
  ],
  "postCreateCommand": "flutter pub get && flutter precache"
}
